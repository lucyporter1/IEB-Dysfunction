#Run DE (on normalized epithelial cells ony, see "UMAP"):
kong_CD_diagnosis <- subset(epithelial_kong, subset = disease__ontology_label == "CD") 
kong_Non_IBD_diagnosis <- subset(epithelial_kong, subset = disease__ontology_label == "Non-IBD")


Idents(epithelial_kong) <- kong$disease__ontology_label
gsea_markers_kong <- FindMarkers(kong, ident.1 = "CD", ident.2 = "Non-IBD", min.pct = 0.5)
write.table(de_markers_kong, file = "kongde.txt", row.names = TRUE, col.names = TRUE, quote = FALSE)


